  <div id="category-container">
    <ul>
      {{#each categories}}
        <li id="{{id}}" class="category-link"><a>{{category_name}}</a></li>
      {{/each}}
    </ul>
  </div>

  <div id="search-result-container">
    {{! once a category is clicked, joke cards go here }}
  </div>
</div>

<script src="/js/search.js"></script>
